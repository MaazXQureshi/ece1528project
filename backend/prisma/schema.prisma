generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Reading {
  id          Int      @id @default(autoincrement())
  volume      Float
  angle       Float
  temperature Float
  time        DateTime @default(now())
}

model Threshold {
  id        Int    @id @default(autoincrement())
  threshold Float
}